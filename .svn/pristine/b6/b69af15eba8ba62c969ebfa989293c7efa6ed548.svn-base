<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ include file="../header.jsp" %>
<%@ include file="mypagesidemenu.jsp" %>   
<div class="mygiveList">
		<h1><span>신청한 글</span></h1>
		<!-- 내가 신청한 교환 -->
	<table class="mygiveList_table">
	<thead>
	<tr>
		<td>기부 재능</td>
		<td>희망 재능</td>
		<td>모집 인원</td>
		<td>제목</td>
		<td>내용</td>
		<td>위치</td>
		<td>마감일</td>
		<td>스크랩</td>
	</tr>
	</thead>
	<c:forEach var="myexchange" items="${myExchangeList}">
		<tr>
			<td>${myexchange.give_talent1}&ensp;/&ensp;${myexchange.give_talent2}</td>
			<td>${myexchange.wish_talent1}&ensp;/&ensp;${myexchange.wish_talent2}</td>
			<td>0&ensp;/&ensp;${myexchange.people}</td>
			<td><a href="#">${myexchange.subject}</a></td>
			<td><a href="#">${myexchange.content}</a></td>
			<td>${myexchange.location}</td>
			<td>${myexchange.closing_date}&ensp;</td>
			<td><input type="checkbox" id="clip"></td>
		</tr>
	</c:forEach>
	</table>
</div>

<div id=member></div>


<%@ include file="../footer.jsp"%>