<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ include file="/admin/admin_header.jsp"%>

<article>
	<h2 class="menu_board">Q&amp;A 관리</h2>
	<div class="table">
		<table class="admin_table">
			<tr>
				<th width=10%>아이디</th>
				<th width=30%>제목</th>
				<th width=45%>내용</th>
				<th width=10%>작성일</th>
				<th width=5%>처리상태</th>
			</tr>
			<c:forEach var="adminQnaList" items="${adminQnaList}">
				<tr>
					<td>${adminQnaList.id}</td>
					<td><a href="/admin/qnaDetail&${adminQnaList.no}">${adminQnaList.subject}</a></td>
					<td><a href="/admin/qnaDetail&${adminQnaList.no}">${adminQnaList.content}</a></td>
					<td>${adminQnaList.cre_date}</td>
					<td>${adminQnaList.repyn}</td>
				</tr>
			</c:forEach>
		</table>
	</div>
</article>

<%@ include file="/admin/admin_footer.jsp"%>