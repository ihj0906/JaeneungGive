
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ include file="../header.jsp" %>
<%@ include file="mypagesidemenu.jsp" %>   

<link href="/css/mypage.css" rel="stylesheet" type="text/css" />

<div class="mygiveList">
		<h1><span>신청한 글</span></h1>
		<!-- 내가 신청한 기부 -->
	<table class="mygiveList_table">
	<thead>
	<tr>
			<td>작성자</td>
			<td>제목</td>
			<td>신청일</td>
			<td>진행 상태</td>
			<td>삭제</td>
			
		</tr>
	</thead>
	<c:forEach var="mydonation" items="${myApplyDonationList}">
		<tr>
			<td>${mydonation.id}</td>
			<td><a href="/give/donationDetail&${mydonation.no}">${mydonation.subject}</a></td>
			<td><fmt:formatDate value="${mydonation.app_appli_date}"/></td>
			<td>${mydonation.app_progress}</td>
			<td><input type="checkbox" id="clip"></td>
			</tr>
	</c:forEach>
	</table>
</div>

<div id=member></div>


<%@ include file="../footer.jsp"%>