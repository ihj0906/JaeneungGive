package service;

import dao.AdminDao;
import dto.Admin;

public class AdminLoginService {
	private AdminDao adminDao;
	
	public AdminLoginService(AdminDao adminDao) {
		this.adminDao = adminDao;
	}
	
	public Admin admin(String id, String password) {
		Admin admin = null;
		
		admin = adminDao.SelectId(id);
		
		if(admin != null) {
			return admin;
		};
		return admin;
	}
}
