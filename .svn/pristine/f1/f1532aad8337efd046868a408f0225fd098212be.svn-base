<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ include file="../header.jsp" %>
<%@ include file="mypagesidemenu.jsp" %>   

<link href="/css/mypage.css" rel="stylesheet" type="text/css" />


<div class="mygiveList">
		<h1><span>내가 쓴 글</span></h1>
		<form name="mypagefrm">
		<!-- 내가 쓴 교환글 -->
	<table class="mygiveList_table">
	
	<thead>
	<tr>
		<td>기부 재능</td>
		<td>희망 재능</td>
		<td>제목</td>
		<td>마감일</td>
		<td>진행 상태</td>
		<td>삭제</td>
		
	</tr>
	</thead>
	<c:forEach var="mywriteexchange" items="${mywriteExchangeList}">
		<tr>
			<td>${mywriteExchange.give_talent1}&ensp;/&ensp;${mywriteExchange.give_talent2}</td>
			<td>${mywriteExchange.wish_talent1}&ensp;/&ensp;${mywriteExchange.wish_talent2}</td>
			<td><a href="/give/exchangeDetail&${mywriteExchange.no}">${mywriteExchange.subject}</a></td>
			<td>${mywriteExchange.closing_date}&ensp;</td>
			<td>${myExchange.app_progress}</td>
			<td><input type="checkbox" id="clip"></td>
		</tr>
	</c:forEach>
	</table>
		<input type="button" id="delete_btn" value="체크 한 글 삭제" onclick="delDonationClasses()">
	</form>
</div>


<div id=member></div>
		
<%@ include file="../footer.jsp"%>