<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ include file="/admin/admin_header.jsp"%>

<article>
	<h2 class="menu_board">Q&amp;A 관리</h2>
	<form method="post" name="form">
		<table class="admin_table">
				<tr>
					<th width=20%>아이디</th>
					<td width=80%>${adminQnaDetail.id}</td>
				</tr>
				<tr>
					<th>제목</th>
					<td>${adminQnaDetail.subject}</td>
				</tr>
				<tr>
					<th>내용</th>
					<td>${adminQnaDetail.content}</td>
				</tr>
				<tr>
					<th>첨부파일</th>
					<td>추가 예정</td>
				</tr>
				<tr>
					<th>작성일</th>
					<td>${adminQnaDetail.cre_date}</td>
				</tr>
		</table>
		<table>
			<tr class="qna_post">
				<td colspan="2"><textarea name="reply" cols="50" rows="5"></textarea>
					<input type="button" value="입력"></td>
			</tr>
		</table>
		<br> <input type="button" value="목록">
	</form>
</article>

<%@ include file="/admin/admin_footer.jsp"%>