html, body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre,
	code, form, fieldset, legend, input, textarea, p, blockquote, th, td,
	img {
	margin: 0;
	padding: 0;
}

html {
	width: 100%;
	height: 100%;
}

body, code {
	font-size: 11px;
	font-family: Nanum Gothic;
	color: black;
	margin: auto;
}

body {
	min-width: 720px;
}

body#popup {
	min-width: 0;
}

li {
	list-style: none;
}

img, fieldset {
	border: none;
	vertical-align: top;
}

table {
	width: 100%;
	border: 0;
	border-spacing: 0;
	border-collapse: collapse;
}

caption {
	display: none;
}

th, td {
	border: 0;
	vertical-align: top;
}

input, select, textarea {
	font-size: 100%;
	color: black;
	vertical-align: middle;
	font-family: Nanum Gothic;
}

input[type=radio], input[type=checkbox] {
	width: 13px;
	height: 13px;
	border: 0;
	font-family: Nanum Gothic;
}

input[type=text], input[type=password] {
	height: 18px;
	line-height: 20px;
	padding: 2px 4px;
	border: 1px solid #ddd;
	color: black;
	font-size: 11px;
	font-family: Nanum Gothic;
}

input[type=radio]+label, input[type=checkbox]+label {
	margin: 0 4px 0 2px;
	font-family: Nanum Gothic;
}

.inputTypeText {
	height: 19px;
	padding: 3px 0 0 9px;
	border: 1px solid #d5d5d5;
}

select {
	padding: 0 30px 0 4px;
	height: 26px;
	border: 1px solid #dddddd;
	background: url("/web/upload/aidelab/select_arrow.png") no-repeat 96%
		50%;
	background-size: 11px;
	font-family: Nanum Gothic;
	appearance: none;
	-webkit-appearance: none;
}

select::-ms-expand {
	display: none;
}

select:focus {
	outline: none;
}

textarea {
	padding: 5px 6px;
	border: 1px solid #dddddd;
	line-height: 1.5;
	font-family: Nanum Gothic;
}

legend {
	visibility: hidden;
	position: absolute;
	left: -9999px;
	top: -9999px;
	width: 0;
	height: 0;
	line-height: 0;
}

button {
	overflow: visible;
	padding: 0;
	margin: 0;
	border: 0;
	cursor: pointer;
}

hr.layout {
	display: none;
}

a {
	text-decoration: none;
	color: black;
}

a:hover {
	color: #999;
}

a:active {
	text-decoration: none;
}

.objHidden {
	visibility: hidden;
	position: absolute;
	left: -1000px;
	top: -1000px;
	height: 0;
	width: 0;
}

table tr.radioType input, span.noBorder input {
	border: none !important;
	width: auto !important;
	height: auto !important;
	margin: 0 3px 0 0 !important;
	vertical-align: middle !important;
